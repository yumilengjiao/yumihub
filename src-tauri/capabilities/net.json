{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "net",
  "description": "控制网络请求的能力",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.vndb.org/*"
        },
        {
          "url": "https://*.bgm.tv/*"
        },
        {
          "url": "https://*.ymgal.games/*"
        }
      ]
    }
  ]
}
